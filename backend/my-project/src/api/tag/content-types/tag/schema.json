{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tag": {
      "type": "string",
      "maxLength": 30,
      "required": true,
      "unique": true,
      "regex": "[a-z\\-]*"
    },
    "programs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::program.program",
      "mappedBy": "tags"
    }
  }
}