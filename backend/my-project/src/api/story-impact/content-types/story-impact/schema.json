{
  "kind": "collectionType",
  "collectionName": "story_impacts",
  "info": {
    "singularName": "story-impact",
    "pluralName": "story-impacts",
    "displayName": "Story/Impact"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "videos",
        "images"
      ]
    },
    "program": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::program.program",
      "inversedBy": "story_impacts"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "beneficiaries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::beneficiary.beneficiary",
      "mappedBy": "story_impacts"
    }
  }
}
